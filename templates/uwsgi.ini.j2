[uwsgi]
chdir = {{wsgi_directory}}
chmod-socket = 666
enable-threads = true
file = {{wsgi_app_path}}
log-master = true
master = true
processes = {{wsgi_uwsgi_processes}}
socket = {{wsgi_uwsgi_socket}}
vaccuum = true
virtualenv = {{wsgi_virtualenv}}

{% if wsgi_uwsgi_reload %}
touch-reload = {{wsgi_directory}}
py-autoreload = true
{% endif %}

